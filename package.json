{
  "name": "puppet-resume",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 src/template.html dist",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:example": "ts-node src/index.ts",
    "dev:example:with-avatar": "ts-node src/index.ts with-avatar",
    "dev:example:without-avatar": "ts-node src/index.ts without-avatar",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Resume PDF generator using TypeScript and Puppeteer",
  "dependencies": {
    "express": "^5.2.1",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^24.34.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.0",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
